<head>

</head>
<h1>Address Book</h1>
@if(Model.Count == 0)
{
  <h3>You don't have any contacts yet. Click below to add one.</h3>
}
<ul>
@foreach(var contact in Model)
{
  <li><a href="/contact/@contact.GetId()">@contact.GetFirstName() @contact.GetLastName()</a></li>
}
<ul>
<p><a href="/contact/new">Add a new contact</a></p>
<p><a href="/contacts/clear">Clear all contacts</a></p>
